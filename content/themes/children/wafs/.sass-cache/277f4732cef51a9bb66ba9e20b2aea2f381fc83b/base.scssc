/* Typographical Elements
--------------------------------------------- */
/* REMS
	The rem values are calculated per the examples below:

	12 / 16 =	0.75rem
	14 / 16 =	0.875rem
	16 / 16 =	1rem	
	18 / 16 =	1.125rem
	20 / 16 =	1.25rem
	24 / 16 =	1.5rem
	30 / 16 =	1.875rem
	36 / 16 =	2.25rem
	42 / 16 =	2.625rem
	48 / 16 =	3rem
	
	Further reading on the use and compatibility of rems:
	http://caniuse.com/rem
	http://snook.ca/archives/html_and_css/font-size-with-rem
*/


html {
  font-size: 62.5%;
  /* 10px browser default */
}

body {
  background: #d4eaff url(../../images/veg.jpg);
  background-size: 100% auto;
  background-repeat:no-repeat;
  color: $medgray;
  font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
  font-size: 16px;
  font-size: 1.6rem;
  font-weight: 300;
  line-height: 1.625;
}

a,
button {
  @include transitionInOut;
}

input {
  &:focus, &[type="button"],
  &[type="reset"],
  &[type="submit"] {
    @include transitionInOut;
  }
}

textarea:focus,
.button {
  @include transitionInOut;
}

::-moz-selection,
::selection {
  background-color: #1e1e1e;
  color: $white;
}

a {
  color: $medgray;
  text-decoration: none;
  img {
    margin-bottom: -4px;
    margin-bottom: -0.4rem;
  }
  &:hover {
    color: $darkgray;
  }
}

p {
  margin: 0 0 24px;
  margin: 0 0 2.4rem;
  padding: 0;
}

strong {
  font-weight: 700;
}

ol, ul {
  margin: 0;
  padding: 0;
}

blockquote {
  color: $gray;
  &::before {
    color: $gray;
    content: "\201C";
    display: block;
    font-size: 30px;
    font-size: 3rem;
    height: 0;
    left: -20px;
    position: relative;
    top: -10px;
  }
  margin: 40px 40px 24px;
  margin: 4rem 4rem 2.4rem;
}

.entry-content code {
  background-color: $darkgray;
  color: #ddd;
}

cite {
  font-style: normal;
}

/* Headings
--------------------------------------------- */

h1, .h1,
h2, .h2,
h3, .h3,
h4, .h4,
h5, .h5,
h6, .h6 {
  color: $darkgray;
  font-family: Lato, sans-serif;
  font-weight: 700;
  line-height: 1.2;
  margin: 0 0 16px;
  margin: 0 0 1.6rem;
}

h1, .h1 {
  font-size: 36px;
  font-size: 3.6rem;
}

h2, .h2 {
  font-size: 30px;
  font-size: 3rem;
}

h3, .h3 {
  font-size: 24px;
  font-size: 2.4rem;
}

h4, .h4 {
  font-size: 20px;
  font-size: 2rem;
  margin: 0 0 0px;
  margin: 0 0 0rem;
}

h5, .h5 {
  font-size: 18px;
  font-size: 1.8rem;
}

h6, .h6 {
  font-size: 16px;
  font-size: 1.6rem;
}

/* Objects
--------------------------------------------- */

embed,
iframe,
img,
object,
video,
.wp-caption {
  max-width: 100%;
}

img {
  height: auto;
  width: auto;
  /* IE8 */
}

/* Forms
--------------------------------------------- */

input,
select,
textarea {
  background-color: $white;
  border: 1px solid #ddd;
  border-radius: 3px;
  box-shadow: 1px 1px 3px #eee inset;
  color: $gray;
  font-size: 14px;
  font-size: 1.4rem;
  padding: 16px;
  padding: 1.6rem;
  width: 100%;
}

input:focus,
textarea:focus {
  border: 1px solid $gray;
  outline: none;
}

::-moz-placeholder {
  color: $gray;
  opacity: 1;
}

::-webkit-input-placeholder {
  color: $gray;
}

button {
  background-color: $darkgray;
  border: none;
  box-shadow: none;
  color: $white;
  cursor: pointer;
  padding: 16px 24px;
  padding: 1.6rem 2.4rem;
  text-transform: uppercase;
  width: auto;
}

input {
  &[type="button"],
  &[type="reset"],
  &[type="submit"] {
    background-color: $darkgray;
    border: none;
    box-shadow: none;
    color: $white;
    cursor: pointer;
    padding: 16px 24px;
    padding: 1.6rem 2.4rem;
    text-transform: uppercase;
    width: auto;
  }
}

.button,
.entry-content .button {
  background-color: $darkgray;
  border: none;
  box-shadow: none;
  color: $white;
  cursor: pointer;
  padding: 16px 24px;
  padding: 1.6rem 2.4rem;
  text-transform: uppercase;
  width: auto;
}

button:hover {
  background-color: #f15123;
}

input:hover {
  &[type="button"],
  &[type="reset"],
  &[type="submit"] {
    background-color: #f15123;
  }
}

.button:hover {
  background-color: #f15123;
}

.entry-content .button:hover {
  background-color: #f15123;
  color: $white;
}

.button {
  border-radius: 3px;
  display: inline-block;
}

input[type="search"] {
  &::-webkit-search-cancel-button, &::-webkit-search-results-button {
    display: none;
  }
}

/* Tables
--------------------------------------------- */

table {
  border-collapse: collapse;
  border-spacing: 0;
  line-height: 2;
  margin-bottom: 40px;
  margin-bottom: 4rem;
  width: 100%;
}

tbody {
  border-bottom: 1px solid #ddd;
}

th,
td {
  text-align: left;
}

th {
  font-weight: bold;
  text-transform: uppercase;
}

td {
  border-top: 1px solid #ddd;
  padding: 6px 0;
  padding: 0.6rem 0;
}

/*
Structure and Layout
---------------------------------------------------------------------------------------------------- */

/* Home Welcome
------------------------------------------------------------ */

.home-welcome {
  padding: 20px 30px;
  color: #777;
  font-size: 18px;
  text-align: center;
  text-shadow: 1px 1px $black;
  p {
    color: #777;
    font-size: 18px;
    text-align: center;
    text-shadow: 1px 1px $black;
  }
}

/* Home Slider
------------------------------------------------------------ */

.home-slider {
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  -moz-box-shadow: 0 1px 2px $gray;
  -webkit-box-shadow: 0 1px 2px $gray;
  background-color: $white;
  border-radius: 5px;
  box-shadow: 0 1px 2px $gray;
  margin: 0 auto 20px;
  width: 80%;
  p {
    padding: 0 0 25px;
  }
}

.lad {
  #genesis-responsive-slider {
    -webkit-font-smoothing: antialiased;
    border: none;
    margin: 5px;
    padding: 5px 0;
  }
  .slide-excerpt {
    filter: alpha(opacity = 100);
    margin: 0;
    padding: 22px 40px 22px 20px;
    opacity: 1.0;
  }
}

html > body.lad .slide-excerpt-border {
  border: none;
  margin: 0;
  padding: 0;
}

.lad {
  .slider-next,
  .slider-previous {
    margin: 0 -40px;
  }
  .flex-direction-nav li a {
    margin: -20px 20px 0;
  }
  .flex-control-nav {
    margin: 20px 0;
  }
}

