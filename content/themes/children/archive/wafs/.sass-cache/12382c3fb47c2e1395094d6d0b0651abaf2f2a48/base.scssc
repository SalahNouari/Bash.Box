/* WordPress
--------------------------------------------- */

.avatar {
  float: left;
}

.alignleft .avatar {
  margin-right: 24px;
  margin-right: 2.4rem;
}

.alignright .avatar {
  margin-left: 24px;
  margin-left: 2.4rem;
}

.search-form input[type="submit"] {
  border: 0;
  clip: rect(0, 0, 0, 0);
  height: 1px;
  margin: -1px;
  padding: 0;
  position: absolute;
  width: 1px;
}

.sticky! {}

img.centered,
.aligncenter {
  display: block;
  margin: 0 auto 24px;
  margin: 0 auto 2.4rem;
}

img.alignnone {
  margin-bottom: 12px;
  margin-bottom: 1.2rem;
}

.alignleft {
  float: left;
  text-align: left;
}

.alignright {
  float: right;
  text-align: right;
}

img.alignleft,
.wp-caption.alignleft {
  margin: 0 24px 24px 0;
  margin: 0 2.4rem 2.4rem 0;
}

img.alignright,
.wp-caption.alignright {
  margin: 0 0 24px 24px;
  margin: 0 0 2.4rem 2.4rem;
}

.wp-caption-text {
  font-size: 14px;
  font-size: 1.4rem;
  font-weight: 700;
  text-align: center;
}

.gallery-caption! {}

.widget_calendar {
  table {
    width: 100%;
  }
  td, th {
    text-align: center;
  }
}

/* Genesis
--------------------------------------------- */

.breadcrumb {
  margin-bottom: 20px;
  margin-bottom: 2rem;
}

.archive-description,
.author-box {
  background-color: $white;
  margin-bottom: 40px;
  margin-bottom: 4rem;
}

.archive-description {
  padding: 40px 40px 24px;
  padding: 4rem 4rem 2.4rem;
}

.author-box {
  padding: 40px;
  padding: 4rem;
}

.author-box-title {
  color: $darkgray;
  font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
  font-size: 16px;
  font-size: 1.6rem;
  margin-bottom: 4px;
  margin-bottom: 0.4rem;
}

.author-box {
  a {
    border-bottom: 1px solid #ddd;
  }
  p {
    margin-bottom: 0;
  }
  .avatar {
    margin-right: 24px;
    margin-right: 2.4rem;
  }
}

/* Titles
--------------------------------------------- */

.entry-title {
  font-size: 36px;
  font-size: 3.6rem;
  line-height: 1;
  a {
    color: $darkgray;
  }
}

.sidebar .widget-title a {
  color: $darkgray;
}

.entry-title a:hover {
  color: $medgray;
}

.widget-title {
  font-size: 16px;
  font-size: 1.6rem;
  margin-bottom: 24px;
  margin-bottom: 2.4rem;
  text-transform: uppercase;
}

.sidebar .widget-title a {
  border: none;
}

.archive-title {
  font-size: 20px;
  font-size: 2rem;
  margin-bottom: 24px;
  margin-bottom: 2.4rem;
}

/*
Widgets
---------------------------------------------------------------------------------------------------- */

/* Featured Content
--------------------------------------------- */

.featured-content {
  .entry {
    border-bottom: 2px solid #f5f5f5;
  }
  .entry-title {
    border: none;
    font-size: 24px;
    font-size: 2.4rem;
    line-height: 1.2;
    a {
      border: none;
    }
  }
}

/*
Plugins
---------------------------------------------------------------------------------------------------- */

/* Genesis eNews Extended
--------------------------------------------- */

.enews-widget {
  color: $gray;
  .widget-title {
    color: $white;
  }
  input {
    margin-bottom: 16px;
    margin-bottom: 1.6rem;
    &:focus {
      border: 1px solid #ddd;
    }
    &[type="submit"] {
      background-color: #f15123;
      color: $white;
      margin: 0;
      width: 100%;
    }
    &:hover[type="submit"] {
      background-color: $white;
      color: $darkgray;
    }
  }
}

/* Genesis Latest Tweets
--------------------------------------------- */

.latest-tweets ul li {
  margin-bottom: 16px;
  margin-bottom: 1.6rem;
}

/* Gravity Forms
--------------------------------------------- */

div.gform_wrapper {
  input {
    &[type="email"], &[type="text"] {
      font-size: 16px;
      font-size: 1.6rem;
      padding: 16px;
      padding: 1.6rem;
    }
  }
  textarea {
    font-size: 16px;
    font-size: 1.6rem;
    padding: 16px;
    padding: 1.6rem;
  }
  .ginput_complex label {
    font-size: 16px;
    font-size: 1.6rem;
    padding: 16px;
    padding: 1.6rem;
    padding: 0;
  }
  li, form li {
    margin: 16px 0 0;
    margin: 1.6rem 0 0;
  }
  .gform_footer input[type="submit"] {
    font-size: 14px;
    font-size: 1.4rem;
  }
}

/* Jetpack
--------------------------------------------- */

img#wpstats {
  display: none;
}

/*
Site Header
---------------------------------------------------------------------------------------------------- */

.site-header {
  min-height: 120px;
  overflow: hidden;
  .wrap {
    padding: 40px 0;
    padding: 4rem 0;
  }
}

/* Title Area
--------------------------------------------- */

.title-area {
  float: left;
  font-family: Lato, sans-serif;
  font-weight: 700;
  padding: 16px 0;
  padding: 1.6rem 0;
  width: 320px;
}

.header-image .title-area {
  padding: 0;
}

.site-title {
  font-size: 28px;
  font-size: 2.8rem;
  line-height: 1;
  margin: 0 0 8px;
  margin: 0 0 0.8rem;
  text-transform: uppercase;
  a {
    color: $darkgray;
    &:hover {
      color: $darkgray;
    }
  }
}

.site-description {
  color: $gray;
  font-size: 16px;
  font-size: 1.6rem;
  font-weight: 300;
  line-height: 1;
  margin-bottom: 0;
}

/* Full width header, no widgets */

.header-full-width {
  .title-area,
  .site-title {
    width: 100%;
  }
}

.header-image {
  .site-description,
  .site-title a {
    /* 9999px HACK FIX
    http://www.zeldman.com/2012/03/01/replacing-the-9999px-hack-new-image-replacement/ */
    text-indent: 100%;
    white-space: nowrap;
    overflow: hidden;
  }
  .site-header .wrap {
    background: url(../../images/logo.png) no-repeat left;
    padding: 0;
  }
  .site-title a {
    float: left;
    min-height: 164px;
    width: 100%;
  }
}

/* Logo, hide text */

/* Widget Area
--------------------------------------------- */

.widget-area {
  word-wrap: break-word;
}

.site-header .widget-area {
  float: right;
  text-align: right;
  width: 800px;
}

.header-image .site-header .widget-area {
  padding: 40px 0;
  padding: 4rem 0;
}

.site-header .search-form {
  float: right;
  margin-top: 24px;
  margin-top: 2.4rem;
}

/*
Site Navigation
---------------------------------------------------------------------------------------------------- */

.genesis-nav-menu {
  background-color: rgba(255,255,255,0.9);
  border-radius: 3px;
  clear: both;
  font-family: Lato, sans-serif;
  font-size: 1.4rem;
  font-weight: 600;
  line-height: 1.5;
  overflow: hidden;
  width: 100%;
  .menu-item {
    display: inline-block;
    text-align: left;
  }
  a {
    border: none;
    color: $black;
    display: block;
    padding: 28px 24px;
    padding: 2.8rem 2.4rem;
    position: relative;
    &:hover {
    color: $green;
    }
  }
  .current-menu-item > a {
    color: $black;
  }
  .sub-menu {
    .current-menu-item > a {
      &:hover {
        color: $darkgray;
      }
      color: $gray;
    }
    left: -9999px;
    opacity: 5;
    position: absolute;
    -webkit-transition: opacity .4s ease-in-out;
    -moz-transition: opacity .4s ease-in-out;
    -ms-transition: opacity .4s ease-in-out;
    -o-transition: opacity .4s ease-in-out;
    transition: opacity .4s ease-in-out;
    width: 200px;
    z-index: 99;
    a {
      background-color: $white;
      border: 1px solid #eee;
      border-top: none;
      font-size: 14px;
      font-size: 1.4rem;
      padding: 16px 20px;
      padding: 1.6rem 2rem;
      position: relative;
      width: 200px;
    }
    .sub-menu {
      margin: -54px 0 0 199px;
    }
  }
  > .menu-item > a {
    text-transform: uppercase;
  }
  .menu-item:hover {
    position: static;
    > .sub-menu {
      left: auto;
      opacity: 1;
    }
  }
  > {
    .first > a {
      padding-left: 0;
    }
    .last > a {
      padding-right: 0;
    }
    .right {
      display: inline-block;
      float: right;
      list-style-type: none;
      padding: 28px 0;
      padding: 2.8rem 0;
      text-transform: uppercase;
      > a {
        display: inline;
        padding: 0;
      }
    }
    .rss > a {
      margin-left: 48px;
      margin-left: 4.8rem;
    }
    .search {
      padding: 14px 0 0;
      padding: 1.4rem 0 0;
    }
  }
}

/* Site Header Navigation
--------------------------------------------- */

.site-header .sub-menu {
  border-top: 1px solid #eee;
  .sub-menu {
    margin-top: -55px;
  }
}

/* Primary Navigation
--------------------------------------------- */

.nav-primary {
  a:hover,
  .current-menu-item > a {
    background-color: $white;
    color: $green;
  }
  .sub-menu a:hover {
    color: $green;
  }
}

/* Secondary Navigation
--------------------------------------------- */

.nav-secondary {
  background-color: $white;
}

/*
Content Area
---------------------------------------------------------------------------------------------------- */

/* Entries
--------------------------------------------- */

.entry {
  background-color: $white;
  border-radius: 3px;
  margin-bottom: 16px;
  margin-bottom: 1.6rem;
  padding: 16px 16px 16px;
  padding: 1.6rem 1.6rem 1.6rem;
}

.entry-content {
  a {
    border-bottom: 1px solid #ddd;
    color: #f15123;
    &:hover {
      color: $darkgray;
    }
  }
  .attachment a,
  .gallery a {
    border: none;
  }
  ol, p, ul {
    margin-bottom: 26px;
    margin-bottom: 2.6rem;
  }
}

.quote-caption {
  margin-bottom: 26px;
  margin-bottom: 2.6rem;
}

.entry-content {
  ol, ul {
    margin-left: 40px;
    margin-left: 4rem;
  }
  ol li {
    list-style-type: decimal;
  }
  ul li {
    list-style-type: disc;
  }
  ol ol, ul ul {
    margin-bottom: 0;
  }
  .search-form {
    width: 50%;
  }
}

.entry-meta {
  color: $gray;
  font-size: 14px;
  font-size: 1.4rem;
  a {
    border-bottom: 1px solid #ddd;
  }
}

.entry-header .entry-meta {
  margin-bottom: 24px;
  margin-bottom: 2.4rem;
}

.entry-footer .entry-meta {
  border-top: 2px solid #f5f5f5;
  margin: 0 -40px;
  margin: 0 -4rem;
  padding: 32px 40px 8px;
  padding: 3.2rem 4rem 0.8rem;
}

.entry-comments-link::before {
  content: "\2014";
  margin: 0 6px 0 2px;
  margin: 0 0.6rem 0 0.2rem;
}

.entry-categories,
.entry-tags {
  display: block;
}

/* Pagination
--------------------------------------------- */

.archive-pagination,
.entry-pagination {
  font-size: 14px;
  font-size: 1.4rem;
  margin: 40px 0;
  margin: 4rem 0;
}

.archive-pagination li {
  display: inline;
  a {
    background-color: $darkgray;
    border-radius: 3px;
    color: $white;
    cursor: pointer;
    display: inline-block;
    padding: 8px 12px;
    padding: 0.8rem 1.2rem;
    &:hover {
      background-color: #f15123;
    }
  }
  &.active a {
    background-color: #f15123;
  }
}

.entry-pagination a {
  border-bottom: 1px solid #ddd;
}

/* Comments
--------------------------------------------- */

.comment-respond,
.entry-comments,
.entry-pings {
  background-color: $white;
  margin-bottom: 40px;
  margin-bottom: 4rem;
}

.entry-comments {
  padding: 40px;
  padding: 4rem;
}

.comment-respond,
.entry-pings {
  padding: 40px 40px 16px;
  padding: 4rem 4rem 1.6rem;
}

.comment-respond a,
.entry-comments a {
  border-bottom: 1px solid #ddd;
}

.comment-content {
  clear: both;
}

.entry-comments .comment-author {
  margin-bottom: 0;
}

.comment-respond {
  input {
    &[type="email"], &[type="text"], &[type="url"] {
      width: 50%;
    }
  }
  label {
    display: block;
    margin-right: 12px;
    margin-right: 1.2rem;
  }
}

.comment-list li,
.ping-list li {
  list-style-type: none;
}

.comment-list li {
  margin-top: 24px;
  margin-top: 2.4rem;
  padding: 32px;
  padding: 3.2rem;
  li {
    margin-right: -32px;
    margin-right: -3.2rem;
  }
}

li.comment {
  background-color: #f5f5f5;
  border: 2px solid $white;
  border-right: none;
}

.comment .avatar {
  margin: 0 16px 24px 0;
  margin: 0 1.6rem 2.4rem 0;
}

.entry-pings .reply {
  display: none;
}

.bypostauthor! {}

.form-allowed-tags {
  background-color: #f5f5f5;
  font-size: 14px;
  font-size: 1.4rem;
  padding: 24px;
  padding: 2.4rem;
}

/*
Sidebars
---------------------------------------------------------------------------------------------------- */

.sidebar {
  color: $gray;
  a {
    border-bottom: 1px solid #ddd;
  }
  p:last-child {
    margin-bottom: 0;
  }
  li {
    list-style-type: none;
    margin-bottom: 6px;
    margin-bottom: 0.6rem;
    word-wrap: break-word;
  }
  ul > li:last-child {
    margin-bottom: 0;
  }
  .widget {
    background-color: $white;
    border-radius: 3px;
    margin-bottom: 16px;
    margin-bottom: 1.6rem;
    padding: 16px;
    padding: 1.6rem;
  }
  .enews-widget {
    background-color: $darkgray;
  }
}

/*
Footer Widgets
---------------------------------------------------------------------------------------------------- */

.footer-widgets {
  background-color: $darkgray;
  color: $gray;
  clear: both;
  padding: 40px 0 16px;
  padding: 4rem 0 1.6rem;
}

.footer-widgets-1,
.footer-widgets-3 {
  width: 350px;
}

.footer-widgets-2 {
  width: 360px;
}

.footer-widgets-1 {
  margin-right: 40px;
  margin-right: 4rem;
  float: left;
}

.footer-widgets-2 {
  float: left;
}

.footer-widgets-3 {
  float: right;
}

.footer-widgets {
  a {
    border-bottom: 1px solid $medgray;
    color: $gray;
    &:hover {
      color: #ddd;
    }
  }
  .widget {
    margin-bottom: 24px;
    margin-bottom: 2.4rem;
  }
  .widget-title {
    color: $white;
  }
  li {
    list-style-type: none;
    margin-bottom: 6px;
    margin-bottom: 0.6rem;
    word-wrap: break-word;
  }
  .search-form input:focus {
    border: 1px solid #ddd;
  }
}

/*
Site Footer
---------------------------------------------------------------------------------------------------- */

.site-footer {
  background-color: #d4eaff;
  color: $gray;
  font-size: 14px;
  font-size: 1.4rem;
  padding: 40px 0;
  padding: 4rem 0;
  text-align: center;
  a {
    border-bottom: 1px solid #ddd;
  }
  p {
    margin-bottom: 0;
  }
}
